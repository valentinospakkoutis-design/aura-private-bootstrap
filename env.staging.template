# Staging Environment Variables
# Copy this file to .env.staging and update with your actual values
# DO NOT commit .env.staging to git (add to .gitignore)

# API Configuration
API_BASE_URL=https://staging-api.aura.app
WS_URL=wss://staging-api.aura.app/ws

# Feature Flags
ENABLE_BIOMETRICS=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_WEBSOCKET=true
ENABLE_DARK_MODE=true
ENABLE_OFFLINE_MODE=true

# Debug
DEBUG_MODE=true
LOG_LEVEL=debug

# Analytics
SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id
ANALYTICS_ID=your-staging-analytics-id

# App Version
APP_VERSION=1.0.0-staging
BUILD_NUMBER=1

