# AURA Environment Configuration Template
# Copy this file to .env and update with your values

# Environment (development, staging, production)
EXPO_PUBLIC_ENVIRONMENT=development

# API Configuration
# Development: http://192.168.178.97:8000 (update with your local IP)
# Staging: https://staging-api.aura.com
# Production: https://api.aura.com
EXPO_PUBLIC_API_URL=http://192.168.178.97:8000

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false

# Optional: Backend API Keys (if needed)
# BACKEND_API_KEY=your-backend-api-key

